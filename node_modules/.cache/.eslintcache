length - 1) {\n        return (\n          <li\n            className=\"breadcrumb-item active cn-font\"\n            aria-current=\"page\"\n          >\n            {v}\n          </li>\n        )\n      }\n\n      return (\n        <li className=\"breadcrumb-item cn-font\">\n          <Link to={pathArray.slice(0, i + 1).join('/') + '/' + id }>\n            {v}\n          </Link>\n        </li>\n      )\n    })\n\n    return listArray\n  }\n\n  return (\n    <>\n      <nav aria-label=\"breadcrumb \">\n        <ol className=\"breadcrumb breadcrumb-left\">\n          <li className=\"breadcrumb-item cn-font\">\n            <Link to=\"/\">首頁</Link>\n          </li>\n          {formatText(findPathnameIndex(location.pathname))}\n        </ol>\n      </nav>\n    </>\n  )\n}\n\nexport default withRouter(EventBreadCrumb)\n","/Users/Gary 1/Desktop/Code stuff/資策會/專題/大專/artddict-react/src/pages/event/components/Lightheader.js",[],"/Users/Gary 1/Desktop/Code stuff/資策會/專題/大專/artddict-react/src/pages/map/components/MarqueeMapEnd.js",["1680","1681","1682","1683","1684","1685","1686","1687","1688","1689","1690","1691","1692","1693","1694","1695","1696","1697","1698","1699","1700","1701","1702","1703","1704","1705","1706","1707","1708","1709"],"import React from 'react';\nimport { GiPikeman } from 'react-icons/gi'\nimport { RiArrowLeftSLine } from 'react-icons/ri'\n\nconst MarqueeMapEnd = () => {\n    return (\n        <>\n            <marquee\n            direction=\"right\"\n          scrollamount=\"10\"\n          className=\"event-marquee px-3 my-5\"\n        >\n          <h1 className=\"notoSansTC-md my-3\">\n            探索美學 從周圍開始 <GiPikeman />\n            <RiArrowLeftSLine />\n            <RiArrowLeftSLine />\n            <RiArrowLeftSLine />\n            <RiArrowLeftSLine /> 探索美學 從周圍開始{' '}\n            <GiPikeman />\n            <RiArrowLeftSLine />\n            <RiArrowLeftSLine />\n            <RiArrowLeftSLine />\n            <RiArrowLeftSLine /> 探索美學 從周圍開始{' '}\n      