lick={(e) => {\n                e.preventDefault()\n                setEventClass('D')\n              }}\n            >\n              活動工作坊\n              <IoIosArrowRoundDown />\n            </button>\n          </Row>\n        </Container>\n        <Container className=\"ed-list both-padding\" fluid>\n          <Row className=\"mb-4\">\n            <div\n              className=\"ed-filter col-12 px-0 d-flex justify-content-between my-5 pb-5\"\n              action=\" \"\n            >\n              {/* 地區選單 */}\n              <div\n                className=\"ed-select-box cn-font col-3 p-0 d-flex\n              \"\n              >\n                <h6 className=\"col-5 px-0 text-center\">\n                  地區\n                </h6>\n                <select\n                  className=\"ed-select col-7\"\n                  value={country}\n                  onChange={(e) => {\n                    // 將字串轉成數字\n                    setCountry(+e.target.value)\n                    // 重置township的值\n                    setTownship(-1)\n                    setCity('')\n                  }}\n                  name=\"\"\n                  id=\"\"\n                >\n                  <option value=\"-1\">請選擇</option>\n                  {countries.map((value, index) => (\n                    <option key={index} value={index}>\n                      {value}\n                    </option>\n                  ))}\n                </select>\n              </div>\n\n              {/* 城市選單 */}\n              <div\n                className=\"ed-select-box cn-font col-3 p-0 d-flex\n              \"\n              >\n                <h6 className=\"col-5 px-0 text-center\">\n                  城市\n                </h6>\n                <select\n                  className=\"ed-select col-7\"\n                  value={township}\n                  onChange={(e) => {\n                    // 將字串轉成數字\n                    setTownship(+e.target.value)\n                    setCity(\n                      townships[country][+e.target.value]\n                    )\n                  }}\n                  name=\"\"\n                  id=\"\"\n                >\n                  <option value=\"-1\">請選擇</option>\n                  {country > -1 &&\n                    townships[country].map(\n                      (value, index) => (\n                        <option key={index} value={index}>\n                          {value}\n                        </option>\n                      )\n                    )}\n                </select>\n              </div>\n\n              <div\n                className=\"ed-select-box cn-font col-3 p-0 d-flex\n              \"\n              >\n                <h6 className=\"col-5 px-0 text-center\">\n                  開始日期\n                </h6>\n                <select\n                  className=\"ed-select col-7\"\n                  onChange={(e) => {\n                    // 將字串轉成數字\n                    setOrder(!order)\n                  }}\n                  name=\"\"\n                  id=\"\"\n                >\n                  <option value=\"latest\">由舊至新</option>\n                  <option value=\"oldest\">由新至舊</option>\n                </select>\n              </div>\n\n              <div\n                className=\"ed-list-btn col-1\"\n                onClick={() => {\n                  getEventQueryServer()\n                  // jumpJumpJump()\n                }}\n              >\n                <FaFilter />\n              </div>\n            </div>\n            <div className=\"my-5\"></div>\n          </Row>\n          <Row className=\"ed-list-card justify-content-start flex-wrap cn-font\">\n            {/* test node area */}\n            {eventDisplay}\n            {/* test node area */}\n          </Row>\n\n          <ReactPaginate\n            pageCount={totalPages}\n            pageRangeDisplayed={4}\n            marginPagesDisplayed={0}\n            previousLabel={<IoIosArrowBack />}\n   